<table>

	<tr>

		<th>Original URL</th>
		<th>Shortened URL</th>

	</tr>

	<% @urls.each do |link| %>

		<tr>
		<td> <a href = <%= link.long_url %> > <%= link.long_url %></a></td>
		<td> <a href = <%= link.short_url %> > <%= link.short_url %></a></td>
		<td><%= link_to 'Show', url_path(link) %></td>
		</tr>

		<% end %>

	</table>

